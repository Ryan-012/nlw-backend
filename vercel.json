{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "prisma/schema.prisma",
      "use": "prisma"
    }
  ],
  "routes":  [
    {
      "src": "/auth",
      "dest": "src/routes/auth.ts"
    },
    {
      "src": "/memories",
      "dest": "src/routes/memories.ts"
    },
    {
      "src": "/upload",
      "dest": "src/routes/upload.ts"
    },
    {
      "src": "/user",
      "dest": "src/routes/user.ts"
    },
    {
      "src": "(.*)",
      "dest": "src/server.ts"
    }
  ]
}